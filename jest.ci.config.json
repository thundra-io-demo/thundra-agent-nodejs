{
  "testMatch": [
    "<rootDir>/test/*.test.js",
    "<rootDir>/test/opentracing/*.js",
    "<rootDir>/test/plugins/console.shim.filter.test.js",
    "<rootDir>/test/plugins/console.shim.test.js",
    "<rootDir>/test/plugins/invocation.support.test.js",
    "<rootDir>/test/plugins/invocation.test.js",
    "<rootDir>/test/plugins/invocation.trace.support.test.js",
    "<rootDir>/test/plugins/log-manager.test.js",
    "<rootDir>/test/plugins/log.test.js",
    "<rootDir>/test/plugins/logger.test.js",
    "<rootDir>/test/plugins/metric.test.js",
    "<rootDir>/test/plugins/thundra.config.test.js",
    "<rootDir>/test/plugins/trace.test.js"
  ],
  "transform": {
    ".*": "<rootDir>/node_modules/babel-jest"
  },
  "moduleFileExtensions": [
    "js"
  ],
  "collectCoverageFrom": [
    "dist/*.js",
    "dist/opentracing/*.js",
    "dist/opentracing/instrument/*.js",
    "dist/opentracing/sampler/*.js",
    "dist/plugins/*.js",
    "dist/plugins/data/base/*.js",
    "dist/plugins/data/invocation/*.js",
    "dist/plugins/data/metric/*.js",
    "dist/plugins/config/*.js",
    "dist/plugins/data/trace/*.js",
    "dist/plugins/error/*.js",
    "dist/plugins/listeners/*.js",
    "dist/plugins/support/*.js",
    "index.js",
    "dist/plugins/integrations/*.js"
  ],
  "testURL": "http://localhost/"
}
